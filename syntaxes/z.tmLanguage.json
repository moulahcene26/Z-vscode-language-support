{
	"scopeName": "source.alg",
	"name": "Z Language",
	"patterns": [
	  {
		"name": "keyword.control.zlang",
		"match": "\\b(SOIENT|DEBUT|FIN|ACTION|FONCTION|SI|SINON|FSI|TQ|FTQ|POUR|FPOUR|APPEL|ECRIRE|LIRE|CREER_LISTE|CREER_FILE|EMPILER|DEPILER|ENFILER|DEFILER|AFF_VAL|AFF_ADR|VALEUR|SUIVANT|PILEVIDE|FILEVIDE)\\b"
	  },
	  {
		"name": "storage.type.zlang",
		"match": "\\b(ENTIER|BOOLEEN|CAR|CHAINE|LISTE|PILE|FILE|STRUCTURE|VECTEUR|ARB|ARM)\\b"
	  },
	  {
		"name": "constant.language.boolean.zlang",
		"match": "\\b(VRAI|FAUX|NON)\\b"
	  },
	  {
		"name": "comment.line.zlang",
		"match": "--.*$"
	  },
	  {
		"name": "string.quoted.double.zlang",
		"begin": "\"",
		"end": "\""
	  },
	  {
		"name": "variable.other.zlang",
		"match": "\\b[A-Z][a-zA-Z0-9_]*\\b"
	  },
	  {
		"name": "entity.name.function.zlang",
		"match": "\\b[A-Z][a-zA-Z0-9_]*\\s*(?=\\()"
	  }
	],
	"repository": {}
  }
  