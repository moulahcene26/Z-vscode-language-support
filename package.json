{
  "name": "zlang",
  "publisher": "moulahcene26",
  "displayName": "Z Language Support (ESI)",
  "description": "Z Language support: syntax highlighting and more",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "zlang",
        "aliases": ["Z", "zlang"],
        "extensions": [".alg"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "zlang",
        "scopeName": "source.alg",
        "path": "./syntaxes/z.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "compile": "tsc"
  },
  "icon": "icon.png",
  
  "author": {
  "name": "Moulahene Abdelmounaim",
  "email": "oa_moulahcene@esi.dz"
},
"repository": {
  "type": "git",
  "url": "https://github.com/moulahcene26/Z-vscode-language-support"
}
}
