{
  "name": "zlang",
  "displayName": "z-language-support",
  "description": "Z Language support: syntax highlighting and more",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "z",
      "aliases": ["Z", "z"],
      "extensions": [".alg"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "z",
      "scopeName": "source.z",
      "path": "./syntaxes/z.tmLanguage.json"
    }]
  }
}
