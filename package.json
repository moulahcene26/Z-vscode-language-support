{
  "name": "zlang-support",
  "publisher": "moulahcene26",
  "displayName": "Z Language Support (ESI)",
  "description": "Z Language support: syntax highlighting and more",
  "version": "0.0.7",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "zlang",
        "aliases": [
          "Z",
          "zlang"
        ],
        "extensions": [
          ".alg"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./fileicons/icons/z.svg",
          "dark": "./fileicons/icons/z.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "zlang",
        "scopeName": "source.alg",
        "path": "./syntaxes/z.tmLanguage.json"
      }
    ],
    "iconThemes": [
      {
        "id": "z-file-icon",
        "label": "Z Language File Icons",
        "path": "./fileicons/icon-theme.json"
      }
    ]
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "scripts": {
    "compile": "tsc"
  },
  "icon": "icon.png",
  "author": {
    "name": "Moulahene Abdelmounaim",
    "email": "oa_moulahcene@esi.dz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moulahcene26/Z-vscode-language-support"
  }
}